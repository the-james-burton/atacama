{
  "parserOptions": {
      "ecmaVersion": 6
  },
  "extends": "angular",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "A": true,
    "_": true,
    "d3": true,
    "$": true,
    "SockJS": true,
    "angular": true,
    "module": true,
    "inject": true,
    "moment": true
  },
  "rules": {
    "brace-style": [2, "stroustrup", {"allowSingleLine": true}],
    "camelcase": 1,
    "comma-dangle": [1, "never"],
    "curly": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "indent": [1, 2],
    "lines-around-comment": [2, {"allowBlockStart": true, "beforeBlockComment": true, "beforeLineComment": false}],
    "new-parens": 1,
    "no-bitwise": 1,
    "no-cond-assign": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-empty": 1,
    "no-invalid-regexp": 1,
    "no-invalid-this": 1,
    "no-mixed-spaces-and-tabs": [1, "smart-tabs"],
    "no-multiple-empty-lines": [1, {"max": 2}],
    "no-undef": 1,
    "no-underscore-dangle": 1,
    "no-unreachable": 1,
    "one-var": [1, "never"],
    "quote-props": [1, "as-needed"],
    "semi": [1, "always"],
    "keyword-spacing": [1, { "before": true, "after": true }],
    "space-unary-ops": [1, {"words": true, "nonwords": false}],
    "strict": [1, "function"],
    "wrap-iife": [1, "inside"],
    "yoda": [1, "never"]
  }
}
